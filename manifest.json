{
  "manifest_version": 3,
  "name": "Zero-Inbox Cats for Gmail",
  "version": "0.3",

  "description": "Adds a random cat to the empty state of Gmail inbox",

  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*"],
      "js": ["index.js", "./data/defaults.js", "./utils/index.js"],
      "css": ["styles.css"]
    }
  ]
}
